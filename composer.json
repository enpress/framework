{
    "name": "enpress/framework",
    "description": "The Enpress framework.",
    "keywords": ["enpress", "wordpress", "laravel", "framework"],
    "license": "MIT",
    "authors": [
        {
            "name": "Andris Trieb",
            "email": "andris@darc.co",
            "homepage": "http://developer.darc.co"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ext-mbstring": "*",
        "laravel/framework": "5.6.*",
        "doctrine/inflector": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Enpress\\":"src/Enpress/"
        },
      "files": [
        "src/Enpress/Foundation/helpers.php"
      ]
    },
    "config": {
      "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
